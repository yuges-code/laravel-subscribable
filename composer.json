{
    "name": "yuges-code/laravel-subscribable",
    "description": "ðŸ”” Package for easily attaching subscriptions to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "laravel",
        "package",
        "subscriptions"
    ],
    "homepage": "https://yuges-code.github.io/laravel-subscribable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-subscribable",
        "issues": "https://github.com/yuges-code/laravel-subscribable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "yuges-code/laravel-package": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Subscribable\\": "src",
            "Yuges\\Subscribable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Subscribable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Subscribable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Subscribable\\Providers\\SubscribableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
